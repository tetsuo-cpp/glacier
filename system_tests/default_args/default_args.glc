struct Person {
  string name = "Default";
  int age = 100;

  fn printDetails() -> void {
    print(this.name);
    print(this.age);
  }
};

fn main() -> void {
  let person = new Person("Michelle", 26);
  person.printDetails();
  let person2 = new Person("Alex");
  person2.printDetails();
  let person3 = new Person();
  person3.printDetails();
}